<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.rubenspessoa.shoppingcart"
android:versionCode="1"
android:versionName="1.0" >

<uses-sdk
    android:minSdkVersion="8"
    android:targetSdkVersion="17" />

<application
    android:allowBackup="true"
    android:icon="@drawable/ic_launcher"
    android:label="@string/app_name"
    android:theme="@style/AppTheme" >
    <activity
        android:name="com.rubenspessoa.shoppingcart.MainActivity"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name="com.rubenspessoa.shoppingcart.ListaActivity"
        android:label="@string/title_activity_lista" >
    </activity>
    <activity
        android:name="com.rubenspessoa.shoppingcart.CadastrarProduto"
        android:label="@string/title_activity_cadastrar_produto" >
    </activity>
    <activity
        android:name="com.rubenspessoa.shoppingcart.ListaDeProdutos"
        android:label="@string/title_activity_lista_de_produtos" >
    </activity>
    <activity
        android:name="com.rubenspessoa.shoppingcart.ProdutoActivity"
        android:label="@string/title_activity_produto" >
    </activity>
    <activity
        android:name="com.rubenspessoa.shoppingcart.SugestaoLista"
        android:label="@string/title_activity_sugestao_lista" >
    </activity>
</application>

</manifest>